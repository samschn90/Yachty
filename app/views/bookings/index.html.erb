<h1>All Bookings</h1>
<ul>
  <% @bookings.each do |booking| %>
    <li>Yachtname: <%= booking.yacht.name %></li>
    <li>Begin: <%= booking.start_time %></li>
    <li>End: <%= booking.end_time %></li>
    <li>Interested Person: <%= booking.user.first_name %></li>
    <%= button_to "Accept" %>
    <%= button_to "Decline" %>
  <% end %>
</ul>
